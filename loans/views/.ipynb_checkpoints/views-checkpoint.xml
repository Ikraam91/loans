    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="loans.list">
      <field name="name">loans list</field>
      <field name="model">loans.loans</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->

